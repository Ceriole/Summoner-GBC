{
    "configurations": [
        {
            "name": "GBDK",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/build",
                "${workspaceFolder}/lib/hUGEDriver/include",
                "${env:GBDK_HOME}/include"
            ],
            "defines": [
                "__PORT_sm83",
                "__SDCC",
                "GBDK"
            ],
            "compilerPath": "${env:GBDK_HOME}/bin/lcc.exe",
            "compilerArgs": ["-msm83:gb"],
